{
  "name": "map-management",
  "version": "1.0.0",
  "description": "Map Management Service. Part Of C2 Map System",
  "main": "dist/server.js",
  "scripts": {
    "clean": "rimraf dist",
    "start:dev": "NODE_ENV=development nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "start:production": "NODE_ENV=production node dist/server.js",
    "start:staging": "NODE_ENV=staging node dist/server.js",
    "build": "tsc --listEmittedFiles ",
    "test": "NODE_ENV=development jest --watchAll --coverage --config 'jest.config.js'",
    "test:github-action": "NODE_ENV=development jest --coverage --config 'jest.config.js'",
    "test:postman": "make postman-test-api"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wiormiw/map-management.git"
  },
  "author": "wiormiw",
  "license": "ISC",
  "homepage": "https://github.com/wiormiw/map-management#readme",
  "dependencies": {
    "@fastify/cors": "^7.0.0",
    "@fastify/multipart": "^7.4.2",
    "@sentry/node": "^5.15.5",
    "@types/amqplib": "^0.5.13",
    "@types/fastify-multipart": "^0.7.0",
    "amqplib": "^0.10.3",
    "axios": "^1.3.4",
    "config-yaml": "^1.1.4",
    "dotenv": "^8.2.0",
    "fastify": "^3.7.0",
    "fastify-cors": "^6.1.0",
    "fastify-multer": "^2.0.3",
    "fastify-multipart": "^5.4.0",
    "fastify-plugin": "^3.0.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.29.3",
    "sequelize-typescript": "^2.1.5",
    "ts-node": "^8.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@types/body-parser": "^1.19.0",
    "@types/config-yaml": "^1.1.1",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.52",
    "@types/validator": "^13.7.13",
    "babel-jest": "^26.0.0",
    "jest": "^26.0.0",
    "mock-express-response": "^0.2.2",
    "newman": "^5.0.1",
    "nodemon": "^2.0.3",
    "sequelize-cli": "^6.6.0",
    "typescript": "^3.9.3"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "src/config/config.ts"
    ]
  }
}
